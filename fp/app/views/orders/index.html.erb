<h1>Listing orders</h1>
<%= link_to 'New order', new_order_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Amount</th>
    <th>Size</th>
    <th>Is_oneside</th>
  </tr>
  <% @orders.each do |order| %>
    <tr>
      <td><%= order.name %></td>
      <td><%= order.amount %></td>
      <td><%= order.size %></td>
      <td><%= order.is_oneside %></td>
      <td><%= link_to 'Show', order %></td>
      <td><%= link_to 'Edit', edit_order_path(order) %></td>
    </tr>
  <% end %>
</table>
